<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.RegisterController">
   <children>
      <GridPane layoutX="-1.0" prefHeight="400.0" prefWidth="607.0" style="-fx-background-color: gray; -fx-border-color: orange; -fx-border-width: 5;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#handleRegisterAction" prefHeight="30.0" prefWidth="60.0" style="-fx-border-color: orange; -fx-border-width: 5;" text="Ok" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <TextField fx:id="textField" maxWidth="200.0" prefWidth="100.0" style="-fx-border-color: orange; -fx-border-width: 5;" GridPane.halignment="CENTER" />
            <ChoiceBox fx:id="choiceBox" prefWidth="150.0" style="-fx-border-color: orange; -fx-border-width: 5;" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <Label fx:id="label" prefHeight="43.0" prefWidth="108.0" style="-fx-background-color: orange;" text="Registration" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.valignment="TOP">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Label>
            <PasswordField fx:id="passwordField" maxWidth="200.0" style="-fx-border-color: orange; -fx-border-width: 5;" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
