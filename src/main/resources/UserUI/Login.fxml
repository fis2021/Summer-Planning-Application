<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="408.0" prefWidth="612.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <children>
      <GridPane layoutX="-8.0" layoutY="-7.0" prefHeight="411.0" prefWidth="610.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button onAction="#handleLoginAction" mnemonicParsing="false" text="Login" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <Button mnemonicParsing="false" text="Register" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <Button mnemonicParsing="false" text="Exit" GridPane.halignment="LEFT" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <ChoiceBox fx:id ="choiceBox" prefWidth="150.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <PasswordField fx:id ="passwordField" maxWidth="200.0" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM" />
            <TextField fx:id ="textField" maxWidth="200.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <Label fx:id ="label" prefHeight="29.0" prefWidth="41.0" text="Login" GridPane.halignment="CENTER" GridPane.valignment="TOP" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
